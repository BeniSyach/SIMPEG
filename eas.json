{
  "cli": {
    "version": ">= 7.6.0"
  },
  "build": {
    "amazon-production": {
      "android": {
        "credentialsSource": "local"
      },
      "channel": "amazon-production"
    },
    "google-production": {
      "android": {
        "credentialsSource": "remote"
      },
      "channel": "google-production"
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      },
      "channel": "preview"
    },
    "production": {
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
